{
  "name": "engage-ts-api",
  "version": "1.0.0",
  "description": "A typescript REST api",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "test": "cross-env NODE_ENV=test PORT=3001 jest --testTimeout=10000 --detectOpenHandles --force-exit",
    "dev": "nodemon ./src/server.ts",
    "start": "ts-node ./src/server.ts",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npx sequelize-cli db:create"
  },
  "author": "manish",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "sequelize": "^6.6.4",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@types/express": "^4.17.12",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.12.4",
    "@types/node-cache": "^4.2.5",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "jest": "^27.2.4",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
